                                               Value="@Value" Minimum="0" Maximum="100" StartAngle="220"
                                               EndAngle="140" SegmentCount="50" GapWidth="5"
                                               CornerRadius="CornerType.Square"
                                               Theme="0"
                                               TrackColor="@ColorTrack">
                                    <ProgressBarAnnotations>
